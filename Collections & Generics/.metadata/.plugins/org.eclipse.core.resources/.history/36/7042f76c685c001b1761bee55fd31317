package com.cybage;
import java.util.Comparator;
 
public class NameSort implements Comparator<Employee> 
{
    @Override
    public int compare(Employee o1, Employee o2) {
        return o1.getFirstName().equals(o2.getFirstName());
    }
}


//o2.getFirstName().compareToIgnoreCase(o1.getFirstName());